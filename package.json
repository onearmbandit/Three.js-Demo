{
  "name": "xount",
  "version": "1.0.0",
  "scripts": {
    "clean": "rm -rf themes/xount/dist/*",
    "start": "NODE_ENV=development NODE_SITE=xount node ./webpack/start.js",
    "build": "NODE_ENV=production NODE_SITE=xount node ./webpack/build.js",
    "sniff:setup": "chmod +x sniff_setup.sh && ./sniff_setup.sh",
    "docker:start": "docker-compose down && COMPOSE_HTTP_TIMEOUT=200 docker-compose up --build",
    "docker:setup": "docker exec -it xount-php sh -c /setup.sh",
    "docker:shell": "docker exec -it xount-php /bin/bash",
    "update-deps": "rm -rf node_modules && yarn install"
  },
  "dependencies": {
